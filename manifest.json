{
    "manifest_version": 2,
    "name": "CSP Trolling",
    "version": "1.0",
    "description": "Trolling my CSP Friends cuz im evil",
    "page_action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://dylanp369.github.io/prime/*"
            ],
            "web_accessible_resources": [
                "styles.css"
            ]
        }
    ]
}